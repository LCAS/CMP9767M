<launch>
  <!-- launch sim -->
  <include file ="$(find /uol_cmp9767m_base/launch/thorvald-sim.launch)">
    <env name="DISPLAY" value="" />
      <arg name="gui" value="false" />
  </include>
  <!-- launch map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find /uol_cmp9767m_base/maps/cropped.yaml)" />
  <!-- unit test -->
  <test pkg="Assessment" type="testimage.py" test-name="testimage" />
  <!-- 2nd unit test-->
  <test pkg="Assessment" type="testmove.py" test-name="testmove" />
  <!-- test for new topics -->
  <test name="publishtest"
	test-name="publishtest"
	pkg="rostest" type="publishtest">
    <rosparam>
      topics:
	-name: /colorval_camera
	  timeout: 10
	  negative: False
    </rosparam>
  </test>
</launch>
